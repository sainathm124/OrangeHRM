package com.Arrays;

public class MultiDimensionalArrayExample {
	
	public static void main(String[] args) {
			//Multi Dimensional array 
		//Syntax: datatype arrayname[][]=new datatype[][];
		
		String array1[][]=new String[2][2];
		array1[0][0]="Venkat";
		array1[0][1]="Manual";
		array1[1][0]="srini";
		array1[1][1]="Selenium";
		
	}
}
